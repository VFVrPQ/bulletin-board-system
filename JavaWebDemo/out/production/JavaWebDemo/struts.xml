<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="true"/>
    <constant name="struts.devMode" value="true"/>
    <constant name="struts.ui.theme" value="simple"/>
    <constant name="struts.multipart.maxSize" value="100000000"/>
    <constant name="struts.serve.static.browserCache" value="false"/>
    <constant name="struts.multipart.saveDir" value="/tmp"/>
    
    <package name="new" namespace="/" extends="json-default">
        <action name="user_*" class="userAction" method="{1}">
            <result name="REGISTER_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="LOGIN_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="EXIST_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="GET_DATA_SUCCESS" type="json">
                <param name="root">userEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
        </action>
        <action name="note_*" class="noteAction" method="{1}">
            <result name="ADD_SUCCESS" type="json">
                <param name="root">noteEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="ADD_FAIL" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="DEL_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="GET_SUCCESS" type="json">
                <param name="root">noteEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="UPDATE_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
        </action>
        <action name="userNote_*" class="userNoteAction" method="{1}">
            <result name="ADD_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="DEL_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="GET_SUCCESS" type="json">
                <param name="root">userNoteEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
            <result name="UPDATE_SUCCESS" type="json">
                <param name="root">result</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
        </action>
        <action name="noteList_*" class="noteListAction" method="{1}">
            <result name="GET_SUCCESS" type="json">
                <param name="root">noteListEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
        </action>
        <action name="comment_*" class="commentAction" method="{1}">
            <result name="GET_SUCCESS" type="json">
                <param name="root">commentEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
        </action>
        <action name="search_*" class="searchAction" method="{1}">
            <result name="GET_SUCCESS" type="json">
                <param name="root">noteListEntityList</param>
                <param name="excludeNullProperties">true</param>
                <param name="excludeProperties">SUCCESS</param>
            </result>
        </action>
    </package>
</struts>